/ {
    aliases {
        rotary-a = &gpio0;
        rotary-b = &gpio0;
        button = &gpio0;
    };
};

&zephyr_udc0 {
    status = "okay";
    
    hid0: hid_dev_0 {
        compatible = "zephyr,hid-device";
        label = "HID0";
        in-report-size = <16>;
        in-polling-period-us = <1000>;
    };
};

&gpio0 {
    status = "okay";
};